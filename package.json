{
    "name": "ban-client",
    "version": "0.0.0",
    "private": true,
    "description": "A client to manage Minecraft's ban records",
    "keywords": [
        "client",
        "ban-manager",
        "minecraft"
    ],
    "homepage": "https://github.com/psac-serve/cli#readme",
    "bugs": {
        "url": "https://github.com/psac-serve/cli/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/psac-serve/cli.git"
    },
    "license": "MIT",
    "author": "P2P-Develop (https://p2p-dev.team)",
    "main": "src/manager-instance.ts",
    "bin": {
        "psac": "bin/run"
    },
    "files": [
        "bin",
        "lib"
    ],
    "scripts": {
        "build": "tsc -p .",
        "fix": "eslint --fix .",
        "preinstall": "npx only-allow pnpm",
        "lint": "npm-run-all -p lint:*",
        "lint:js": "eslint .",
        "lint:md": "remark -r .remarkrc.js .",
        "prepack": "rm -rf lib && tsc -b && oclif-dev readme",
        "start": "nodemon",
        "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
        "posttest": "eslint . --ext .ts --config .eslintrc",
        "typedoc": "typedoc --out typedoc src",
        "version": "oclif-dev readme && git add README.md"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "src/**/*.ts": [
            "eslint . --fix",
            "git add"
        ],
        "package.json": [
            "sort-package-json"
        ]
    },
    "dependencies": {
        "@oclif/command": "^1.8.0",
        "@oclif/config": "^1.17.0",
        "@oclif/errors": "1.3.4",
        "@oclif/plugin-autocomplete": "0.3.0",
        "@oclif/plugin-help": "3.2.2",
        "@oclif/plugin-not-found": "1.2.4",
        "axios": "^0.21.1",
        "chalk": "^4.1.0",
        "cli-cursor": "^3.1.0",
        "cli-table3": "^0.6.0",
        "cli-truncate": "^2.1.0",
        "command-line-args": "^5.1.1",
        "dot-object": "2.1.4",
        "figures": "^3.2.0",
        "fs-extra": "^9.1.0",
        "i18n": "^0.13.2",
        "lexing": "0.9.0",
        "msgpack": "^1.0.3",
        "ora": "^5.3.0",
        "pretty-error": "^3.0.3",
        "readline-sync": "^1.4.10",
        "repeat-string": "^1.6.1",
        "rotating-file-stream": "^2.1.4",
        "slice-ansi": "^4.0.0",
        "spinnies": "0.5.1",
        "sprintf-js": "^1.1.2",
        "string-width": "^4.2.0",
        "strip-ansi": "^6.0.0",
        "sudo-block": "^3.0.0",
        "supports-color": "^8.1.1",
        "terminal-kit": "1.45.9",
        "tslib": "^2.1.0",
        "uuid": "8.3.2",
        "wrap-ansi": "^7.0.0"
    },
    "devDependencies": {
        "@oclif/dev-cli": "^1.26.0",
        "@oclif/test": "^1.2.8",
        "@types/chai": "^4.2.14",
        "@types/cli-spinners": "1.3.0",
        "@types/command-line-args": "^5.0.0",
        "@types/dot-object": "2.1.2",
        "@types/fs-extra": "^9.0.6",
        "@types/i18n": "^0.12.0",
        "@types/mocha": "^8.2.0",
        "@types/msgpack": "^0.0.30",
        "@types/node": "^14.14.25",
        "@types/readline-sync": "^1.4.3",
        "@types/repeat-string": "^1.6.0",
        "@types/sprintf-js": "^1.1.2",
        "@types/supports-color": "^7.2.0",
        "@types/terminal-kit": "1.28.2",
        "@types/uuid": "8.3.0",
        "@types/wrap-ansi": "^3.0.0",
        "@typescript-eslint/eslint-plugin": "^4.15.0",
        "@typescript-eslint/parser": "^4.15.0",
        "chai": "^4.3.0",
        "eslint": "^7.19.0",
        "eslint-plugin-eslint-comments": "3.2.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-json": "^2.1.2",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "4.2.1",
        "eslint-plugin-radar": "0.2.1",
        "eslint-plugin-sort-class-members": "1.9.0",
        "eslint-plugin-sort-destructure-keys": "1.3.5",
        "eslint-plugin-sort-keys-fix": "1.1.1",
        "eslint-plugin-unicorn": "^28.0.0",
        "husky": "^5.0.9",
        "lint-staged": "^10.5.4",
        "mocha": "^8.2.1",
        "nodemon": "^2.0.7",
        "npm-run-all": "^4.1.5",
        "nyc": "^15.1.0",
        "remark-cli": "^9.0.0",
        "remark-lint": "^8.0.0",
        "remark-lint-blank-lines-1-0-2": "^3.0.0",
        "remark-lint-heading-whitespace": "^1.0.0",
        "remark-lint-match-punctuation": "^0.2.0",
        "remark-lint-mdash-style": "^1.1.1",
        "remark-lint-no-chinese-punctuation-in-number": "^0.1.1",
        "remark-lint-no-dead-urls": "^1.1.0",
        "remark-lint-no-empty-sections": "^4.0.0",
        "remark-lint-no-url-trailing-slash": "^3.0.1",
        "remark-lint-spaces-around-word": "^0.1.1",
        "remark-preset-lint-recommended": "^5.0.0",
        "sort-package-json": "^1.48.1",
        "ts-node": "^9.1.1",
        "typedoc": "^0.20.23",
        "typescript": "^4.1.4"
    },
    "engines": {
        "node": ">= 8.0.0",
        "pnpm": ">= 3.0.0"
    },
    "publishConfig": {
        "main": "lib/manager-instance.js",
        "types": "lib/index.d.ts"
    },
    "oclif": {
        "bin": "psac"
    }
}
